<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdinit.icecream.common.workflow.mapper.WfNodeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zdinit.icecream.common.workflow.entity.WfNode">
        <id column="id" property="id" />
        <result column="node_name" property="nodeName" />
        <result column="deal_role" property="dealRole" />
        <result column="deal_role_id" property="dealRoleId" />
        <result column="flow_id" property="flowId" />
        <result column="previous_node_id" property="previousNodeId" />
        <result column="next_node_id" property="nextNodeId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, node_name, deal_role, deal_role_id, flow_id, previous_node_id, next_node_id
    </sql>

</mapper>
